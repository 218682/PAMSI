struct DFSs
{
  bool *odwiedzone;
  lista2s<int> S;
  DFSs()
  { odwiedzone = new bool[G.getSizeVertex];
    for(int i=0; i< V; i++) visited[i]=false; }
  void path(Graf, int, int);
}
void pathDFS(Graf G, int A, int B)
{
  lista<edges> incydencje;
  setLabel(A);
  S.add(A);
  if(A==B)
    return S;
  incydencje=getIncydentEdges(A);
  for(int i=0; i<incydencje.size(); i++)
    {
      if(getLabelE(e)==true)
	w=getNextVertex(A,e);


}
}
lista2s<int> DFS::path(Graf G, int V, int A)
{
  lista<vertex> sasiedzi;
  odwiedzone[V]=true;
  S.add(V);
  if(V==A)
    return S;
  sasiedzi=G.getNeighbors(V);
  for(int i=0; i<sasiedzi.size(); i++)
    if(odwiedzone[getVertex.lp]==false)
      DFS(G, i, A);
}
